{
  "name": "punit-shah.github.io",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://punit-shah.github.io",
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.1",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-dom": "^16.8.1",
    "react-scripts": "2.1.5",
    "source-map-explorer": "^1.7.0"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
